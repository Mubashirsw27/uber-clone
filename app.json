{
  "expo": {
    "name": "UberClone",
    "slug": "UberClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "UberClone.com"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Hello Everyone!!! It's a Uber App Clone Built from Scratch by Mubashir Ibrahim. \nYou can contact me on my Social Media Accounts, If you want any Help.\nThanks "
  }
}
